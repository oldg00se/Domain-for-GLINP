(define (problem pro-Diagonal-Return)
        (:domain Diagonal-Return)
        (:init
            (and
              (not (visitlt))
              (not (visitrt))
              (not (visitrb))
              (>= (disr) 0)
              (>= (disl) 0)
              (>= (dist) 0)
              (>= (disb) 0)
              (= (startl) (disl))
              (= (startt) (dist))
            )
        )
        (:goal (and
                   (visitlt)
                   (visitrt)
                   (visitrb)
                   (= (disl) (startl))
                   (= (dist) (startt))
               )
        )
)